{
    "name": "tcgunel/netgsm",
    "description": "Laravel Netgsm channel entegrasyonu.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tolga Can GÃœNEL",
            "email": "info@mobius.studio"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "guzzlehttp/guzzle": "*",
        "illuminate/support": "*",
        "ext-soap": "*",
        "ext-xmlwriter": "*",
        "ext-curl": "*",
        "illuminate/database": "*",
        "illuminate/http": "*",
        "illuminate/notifications": "*"
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "TCGunel\\Netgsm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TCGunel\\Netgsm\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TCGunel\\Netgsm\\NetgsmServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.12"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
