{
    "name": "tcgunel/netgsm",
    "description": "Laravel Netgsm channel entegrasyonu.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tolga Can GÃœNEL",
            "email": "info@mobius.studio"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "*",
        "illuminate/notifications": "*",
        "illuminate/database": "*",
        "illuminate/support": "*",
        "ext-soap": "*",
        "ext-xmlwriter": "*",
        "ext-curl": "*",
        "codedredd/laravel-soap": "*",
        "ext-simplexml": "*",
        "laravel/legacy-factories": "*"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "TCGunel\\Netgsm\\": "src/",
            "TCGunel\\Netgsm\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TCGunel\\Netgsm\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TCGunel\\Netgsm\\NetgsmServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/paratest --processes 6",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*",
        "brianium/paratest": "*"
    }
}
